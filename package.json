    "builtin-modules": "^3.2.0",
    "esbuild": "0.13.12",
    "obsidian": "latest",
    "rimraf": "^3.0.2",
    "tslib": "2.3.1",
    "typescript": "4.4.4"
  },
  "dependencies": {
    "xml2js": "^0.5.0"
  },
  "peerDependencies": {
    "obsidian-dataview": ">=0.5.0"
  },
  "engines": {
    "node": ">=16.0.0"
