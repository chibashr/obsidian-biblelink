{
  "name": "obsidian-biblelink",
  "version": "1.0.0",
  "description": "Simple Bible study with ASV, SQLite/XML imports, and Literal Word links.",
  "main": "main.js",
  "scripts": {
    "build": "node esbuild.config.mjs",
    "dev": "node esbuild.config.mjs --dev",
    "build:production": "node esbuild.config.mjs production",
